[Desktop Entry]
Exec=sh -c "dconf reset -f / && dconf write /org/gnome/desktop/input-sources/sources \"[('xkb', 'br')]\" && dconf write /org/gnome/shell/favorite-apps \"['firefox_firefox.desktop', 'eclipse-jee.desktop', 'org.gnome.Nautilus.desktop']\" && dconf write /org/gnome/shell/extensions/dash-to-dock/show-trash false && dconf write /org/gnome/shell/extensions/dash-to-dock/show-mounts false"
Terminal=false
Type=Application
